{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "last 2 versions",
            "safari >= 7"
          ]
        } 
      }
    ]
  ],  
  "plugins": [
    [
      "prismjs",
      {
        "languages": [
          "javascript",
          "css",
          "scss",
          "markup"
        ],
        "plugins": [
          "line-numbers"
        ],
        "theme": "default",
        "css": true
      }
    ]
  ]
}